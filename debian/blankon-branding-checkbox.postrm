#!/bin/sh
set -e

if [ -f /usr/share/checkbox/gtk/blankon.checkbox-gtk-head.png.divert ]; then
  rm -f /usr/share/checkbox/gtk/blankon.checkbox-gtk-head.png
  dpkg-divert --local --remove --rename /usr/share/checkbox/gtk/blankon.checkbox-gtk-head.png
fi
if [ -f /usr/share/checkbox/gtk/blankon.checkbox-gtk.glade.divert ]; then
  rm -f /usr/share/checkbox/gtk/blankon.checkbox-gtk.glade
  dpkg-divert --local --remove --rename /usr/share/checkbox/gtk/blankon.checkbox-gtk.glade
fi
