#!/bin/sh
set -e

if [ -f /usr/share/cinnamon/theme/menu.png.divert ]; then
  rm -f /usr/share/cinnamon/theme/menu.png
  dpkg-divert --local --remove --rename /usr/share/cinnamon/theme/menu.png
fi
if [ -f /usr/share/cinnamon/js/ui/menu.js.divert ]; then
  rm -f /usr/share/cinnamon/js/ui/menu.js
  dpkg-divert --local --remove --rename /usr/share/cinnamon/js/ui/menu.js
fi
